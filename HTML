function multiplicationTable() {
    let table = '';

    for (let i = 1; i <= 10; i++) {
        for (let j = 1; j <= 10; j++) {
            table += (i * j).toString().padStart(4, ' '); // Выравнивание чисел для читабельности
        }
        table += '\n'; // Переход на новую строку
    }

    alert(table);
}

multiplicationTable();
